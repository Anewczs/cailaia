page {
  background: #ededed; }

view {
  padding: 0 20rpx;
  background: #ffffff; }

.logo {
  text-align: center;
  padding-top: 50rpx; }
  .logo image {
    width: 145rpx;
    height: 200rpx; }

.slogan {
  text-align: center;
  padding: 30rpx 0 60rpx; }
  .slogan text {
    color: #666666;
    font-size: 26rpx; }

.search {
  text-align: center;
  padding-bottom: 60rpx;
  margin-bottom: 30rpx;
  height: 60rpx; }
  .search navigator {
    border: 2rpx solid #f9bb18;
    border-radius: 30rpx;
    font-size: 28rpx;
    color: #f9bb18;
    padding: 15rpx 180rpx; }

.title {
  margin-bottom: 10rpx;
  padding: 10rpx 0; }
  .title text {
    padding-left: 15rpx;
    margin-left: 20rpx;
    border-left: 4rpx solid #f9bb18; }

.img-list {
  padding: 20rpx; }

.img {
  margin-bottom: 15rpx;
  text-align: center;
  position: relative;
  padding: 0;
  border: 2rpx solid #ddd; }
  .img .price {
    position: absolute;
    top: 0;
    left: 0;
    width: 365rpx;
    font-size: 24rpx;
    background: #f9bb18;
    color: #ffffff;
    padding: 10rpx 5rpx;
    text-align: left; }
    .img .price text {
      display: inline-block;
      width: 50%; }
    .img .price .line {
      position: absolute;
      top: 50%;
      left: 5rpx;
      display: block;
      width: 50%;
      border-bottom: 1px solid #ffffff; }
  .img image {
    width: 100%;
    height: 240px; }
  .img .text {
    display: block;
    padding: 20rpx 10rpx;
    font-size: 30rpx;
    text-align: left;
    border-top: 1px solid #dddddd;
    position: relative; }
    .img .text .color {
      width: 10px;
      height: 10px;
      margin-left: 10px;
      margin-right: 5px;
      display: inline-block; }
    .img .text text {
      color: #666666;
      font-size: 26rpx; }
    .img .text .color1 {
      background: #9c3040; }
    .img .text .color2 {
      background: #4d71a5; }
    .img .text .details {
      position: absolute;
      right: 10rpx;
      top: 15rpx;
      padding: 5rpx;
      border: 2rpx solid #f9bb18;
      border-radius: 4rpx;
      color: #f9bb18; }
